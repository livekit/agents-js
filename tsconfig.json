{
  "compilerOptions": {
    "lib": ["ES2022"],
    "target": "ES2022",
    "module": "Node16",
    "moduleDetection": "force",
    "moduleResolution": "Node16",
    "declaration": true,
    "declarationMap": true,
    "allowJs": false,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true
  },
  "typedocOptions": {
    "entryPoints": ["agents", "plugins/*"],
    "entryPointStrategy": "packages",
    "name": "LiveKit Agents",
    "excludeInternal": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "excludeExternals": true,
    "includeVersion": true,
    "out": "docs",
    "theme": "default"
  }
}
